{
    "name": "@react-ssr-pack/server",
    "version": "1.0.4",
    "description": "Server for listening a React SSR application based on React v18 and @react-ssr-pack",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "author": "Anton Frolov",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:frolant/react-ssr-pack.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "browserslist": [
        "last 2 versions"
    ],
    "scripts": {
        "build": "node ../../run build server isNodeTarget",
        "watch:to:lib": "node ../../run watch-to-lib server isNodeTarget",
        "watch:to:app": "node ../../run watch-to-app server isNodeTarget",
        "watch:to:app:types": "node ../../run watch-to-app-types server isNodeTarget"
    },
    "dependencies": {
        "express": "^4.17.2",
        "prom-client": "^14.0.1",
        "redux": "^4.2.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    }
}
